/! BEGIN Alerts !
#quickview-alerts.tab-pane.fade.no-padding
  #alerts.view-port.clearfix
    /! BEGIN Alerts View !
    .view.bg-white
      /! BEGIN View Header !
      .navbar.navbar-default.navbar-sm
        .navbar-inner
          /! BEGIN Header Controler !
          a.inline.action.p-l-10.link.text-master data-navigate="view" data-view-animation="push-parrallax" data-view-port="#chat" href="javascript:;" 
            i.pg-more
          /! END Header Controler !
          .view-heading
            | Notications
          /! BEGIN Header Controler !
          a.inline.action.p-r-10.pull-right.link.text-master href="#" 
            i.pg-search
          /! END Header Controler !
      /! END View Header !
      /! BEGIN Alert List !
      .list-view.boreded.no-top-border data-init-list-view="ioslist" 
        /! BEGIN List Group !
        .list-view-group-container
          /! BEGIN List Group Header!
          .list-view-group-header.text-uppercase
            | Calendar
          /! END List Group Header!
          ul
            /! BEGIN List Group Item!
            li.alert-list
              /! BEGIN Alert Item Set Animation using data-view-animation !
              a data-navigate="view" data-view-animation="push-parrallax" data-view-port="#chat" href="javascript:;" 
              p.col-xs-height.col-middle
                span.text-warning.fs-10
                  i.fa.fa-circle
              p.p-l-10.col-xs-height.col-middle.col-xs-9.overflow-ellipsis.fs-12
                span.text-master David Nester Birthday
              p.p-r-10.col-xs-height.col-middle.fs-12.text-right
                span.text-warning
                  | Today
                  br/
                span.text-master All Day
              /! END Alert Item!
              /! BEGIN List Group Item!
            /! END List Group Item!
            /! BEGIN List Group Item!
            li.alert-list
              /! BEGIN Alert Item Set Animation using data-view-animation !
              a data-navigate="view" data-view-animation="push-parrallax" data-view-port="#chat" href="#" 
